{
	"expo": {
		"name": "savify",
		"slug": "savify",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.kubacienciala.savify",
			"usesAppleSignIn": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.kubacienciala.savify"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"expo-apple-authentication",
			"expo-font",
			[
				"expo-build-properties",
				{
					"ios": {
						"deploymentTarget": "13.4"
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
